<MudItem xs="12" sm="6" md="12">
    <FieldLabel>Patients</FieldLabel>
    <SfAutoComplete TValue="Guid" TItem="PatientListDto" Placeholder="Select a pateint" DataSource="@PatientList">
        <AutoCompleteFieldSettings Value="Id" Text="FullName"></AutoCompleteFieldSettings>
        <AutoCompleteEvents TValue="Guid" TItem="PatientListDto" ValueChange="OnChangePatient"></AutoCompleteEvents>
    </SfAutoComplete>
</MudItem>

@code {
    [Parameter] public List<PatientListDto> PatientList { get; set; } = new();
    [Parameter] public EventCallback<ChangeEventArgs<Guid, PatientListDto>> OnChangePatient { get; set; }

}