@using Dexterity.Components.Pages.Consultation.Documents.Letter
@using Dexterity.Components.Pages.Consultation.Documents.ScannedDocuments
@using Dexterity.Components.Pages.Consultation.Documents.Sketche
@using Dexterity.Components.Pages.Consultation.Documents.EReferrals
@using Dexterity.Components.Pages.Consultation.Documents.Forms
<SfTab CssClass="e-tab-custom-class mt-2" HeaderPlacement="HeaderPosition.Left">
    <TabItems>
        <TabItem>
            <ChildContent>
                <TabHeader Text="Letter" IconCss="letter-icon"></TabHeader>
            </ChildContent>
            <ContentTemplate>
                <LetterTab/>
            </ContentTemplate>
        </TabItem>
        <TabItem>
            <ChildContent>
                <TabHeader Text="Documents" IconCss="documents-icon"></TabHeader>
            </ChildContent>
            <ContentTemplate>
                <FileExplorer/>
            </ContentTemplate>
        </TabItem>
        <TabItem>
            <ChildContent>
                <TabHeader Text="Sketches" IconCss="sketch-icon"></TabHeader>
            </ChildContent>
            <ContentTemplate>
                <SketcheDetail/>
            </ContentTemplate>
        </TabItem>
        <TabItem>
            <ChildContent>
                <TabHeader Text="E-Referrals" IconCss="ref-icon"></TabHeader>
            </ChildContent>
            <ContentTemplate>
                <ReferralTab/>
            </ContentTemplate>
        </TabItem>
        <TabItem>
            <ChildContent>
                <TabHeader Text="Forms" IconCss="document-icon"></TabHeader>
            </ChildContent>
            <ContentTemplate>
                <FormTab/>
            </ContentTemplate>
        </TabItem>
    </TabItems>
</SfTab>

